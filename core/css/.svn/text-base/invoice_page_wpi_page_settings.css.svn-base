.wpi_class_pre {
    background: none repeat scroll 0 0 #323232;
    border-left: 10px solid #BE821B;
    color: #FEFFD5;
    font-size: 12px;
    height: 600px;
    line-height: 1.1em;
    margin-top: 17px;
    overflow-x: hidden;
    overflow-y: scroll;
    padding: 6px;
    white-space: pre-wrap;
    width: auto;
}

div.wpi_settings_block {
    background: none repeat scroll 0 0 #F7F7F7;
    border: 1px solid #E7E7E7;
    margin-bottom: 10px;
    overflow: auto;
    padding: 10px;
}

#wpi_tab_plugins {
  padding: 0;
}

td.wpi_premium_feature_intro .header {
    font-size: 18px;
    font-weight: bold;
}

#wpi_plugins_ajax_response {
    background: none repeat scroll 0 0 #FFFFAA;
    border: 1px solid #B6B7B1;
    margin: 5px 0;
    padding: 5px 10px;
}

#wpi_premium_feature_table thead td {
    padding: 10px 0;
}

td.wpi_premium_feature_intro {
    border-bottom: 1px solid #CCCCCC;
    padding-top: 30px;
}

#wpi_premium_feature_table {
  width: 100%;
}
td.wpi_premium_feature_image {
    width: 250px;
}
.wpi_premium_feature_image img {
    padding: 20px 10px 20px 20px;
}

#wpi_premium_feature_table .wpi_box {
    margin: 20px;
}

.wpi_box {
    background: none repeat scroll 0 0 #EEEEEE;
    border: 1px solid #DDDDDD;
    border-radius: 7px 7px 7px 7px;
    margin-bottom: 15px;
}

.wpi_box_header {
    border-bottom: 3px solid #E4E4E4;
    padding: 7px 0 10px 11px;
}

.wpi_box_content {
    background: none repeat scroll 0 0 #FFFFFF;
    line-height: 1.2em;
    padding: 10px;
}

.wpi_box_footer {
    padding: 10px;
}

.wpi_box_header p {
    font-size: 11px;
    margin: 0;
}

.wpi_box_header strong {
    color: #000000;
    display: block;
    margin-bottom: 10px;
}

.wpi_features_not_found {
  padding: 10px;
}

#currency-list > div {
  padding-top: 10px;
}

.wpi_settings_list input[type="text"] {
  margin-right: 5px;
}

#wp_invoice_settings_page {
  border: none;
}

#wp_invoice_settings_page ul.wp_invoice_settings_tabs {
  background: none repeat scroll 0 0 transparent;
  border: medium none;
  float: left;
  height: 33px;
  list-style: none outside none;
  margin: 0;
  padding: 0;
  width: 100%;
}

#wp_invoice_settings_page ul.wp_invoice_settings_tabs li {
  background: none repeat scroll 0 0 #F4F4F4;
  border-color: #D0D0D0;
  border-style: solid solid none;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  border-width: 1px 1px 0;
  float: left;
  height: 25px;
  line-height: 23px;
  margin: 0 10px -1px 0;
  overflow: hidden;
  padding: 3px;
  position: relative;
}

#wp_invoice_settings_page ul.wp_invoice_settings_tabs li.ui-tabs-selected {
  background: none repeat scroll 0 0 #FFFFFF;
  border-bottom: 1px solid #FFFFFF;
  height: 26px;
}

#wp_invoice_settings_page ul.wp_invoice_settings_tabs li a {
  color: #000000;
  line-height: 25px;
  outline: medium none;
  padding: 0 10px;
  text-decoration: none;
}

.ui-tabs .ui-tabs-panel.wp_invoice_tab {
  background: none repeat scroll 0 0 #FFFFFF;
  border-color: #DEDEDE;
  border-style: solid;
  border-width: 1px;
  clear: both;
}

.updated.wpi_promotional_notice {
  background: url("images/ud-logo.png") no-repeat scroll right center #FFF1E6;
  border: 1px solid #FFCFAB;
  height: 46px;
  margin-bottom: 5px !important;
}
.updated.wpi_promotional_notice .wpi_promotional_notice_top_line {
  font-family: Helvetica,Arial,Tahoma;
  font-size: 17px;
  font-weight: bold;
  color: #4c484a;
  padding-top: 6px;
}
.updated.wpi_promotional_notice .wpi_promotional_notice_top_line a {
  color: #eb7525;
}
.updated.wpi_promotional_notice .wpi_promotional_notice_top_line a:hover {
  color: #f7924d;
}
.updated.wpi_promotional_notice .wpi_promotional_notice_bottom_line {
  color: #eb7525;
  padding-top: 3px;
}
.updated.wpi_promotional_notice .wpi_promotional_notice_bottom_line a {
  color: #4c484a;
}
.updated.wpi_promotional_notice .wpi_promotional_notice_bottom_line a:hover {
  color: #eb7525;
}